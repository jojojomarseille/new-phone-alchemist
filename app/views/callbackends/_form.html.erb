        <div class="label">formule</div>
        <div class="typo"> <%= f.input :formule, label: false %> </div>

        <div class="label">starttime</div>
        <div class="typo"> <%= f.datetime_field :starttime, label: false %> </div>

        <div class="label">numeros appelé</div>
        <div class="typo"> <%= f.input :numero, label: false %> </div>

        <div class="label">Endtime</div>
